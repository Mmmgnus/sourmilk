{{> head }}

<div class="container">
	{{#unless documentation }}
	<h1> {{ title }} </h1>
	{{/unless}}

	<div class="documentation">
		{{{ documentation }}}
	</div>

	{{#if example}}
		<div class="example-viewer">
			<iframe src="/component/{{name}}"></iframe>
		</div>
	{{/if}}

	{{> navigation}}
</div>

{{> foot }}

